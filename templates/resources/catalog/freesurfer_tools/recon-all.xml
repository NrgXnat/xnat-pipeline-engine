			<pip:name>subjid</pip:name>
			<pip:description>Subject</pip:description>
		</pip:argument>
		<pip:argument id="sd">
			<pip:name>sd</pip:name>
			<pip:description>Subject Dir</pip:description>
		</pip:argument>
		<pip:argument id="i">
			<pip:name>i</pip:name>
			<pip:description>In volume</pip:description>
		</pip:argument>
	</pip:input>
</pip:Resource>
